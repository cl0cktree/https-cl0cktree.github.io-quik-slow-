}

body {
	width:100%;
	height:100%;
	margin:0;
	padding:0;
	border:0;
	background:#fff;
	font-family:'Nanum Gothic','NanumGothic','Gothic','고딕';
	position:relative;
	overflow:auto;
}

img{
	border:0;
}

.all-wrap{
	width:1200px;
	margin:0 auto;
	padding:0;
	border:0;
	position:relative;
	display:block;
}

